<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="15dp"
    app:cardElevation="10dp"
    android:layout_width="match_parent"
    android:layout_margin="8dp"
    android:layout_height="wrap_content"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/cisco_blue"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gabarito"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginBottom="5dp"
                android:padding="10dp"
                android:text="Product | PRI"
                android:textSize="20dp"
                android:textColor="@color/white"
                android:id="@+id/categorydata"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="#A3E7FF"
                >
                <TextView
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/gabarito"
                    android:paddingLeft="10dp"
                    android:text="Goal(USD)"
                    android:textSize="16dp"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/goaldata"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:fontFamily="@font/gabarito"
                    android:padding="10dp"
                    android:textSize="16dp"
                    android:background="@color/white"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="#A3E7FF"
                >
                <TextView
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/gabarito"
                    android:padding="10dp"
                    android:text="Booking"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />
                <TextView
                    android:id="@+id/bookingdata"
                    android:textSize="16dp"
                    android:fontFamily="@font/gabarito"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:layout_weight="0.5"
                    android:background="@color/white"
                    />

            </LinearLayout>
            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
                android:background="#A3E7FF"
            >
                <TextView
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/gabarito"
                    android:padding="10dp"
                    android:text="Non-Comm Bookings"
                    android:textSize="16dp"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/noncomm"
                    android:layout_width="match_parent"
                    android:background="@color/white"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:fontFamily="@font/gabarito"
                    android:padding="10dp"
                    android:textSize="16dp" />

        </LinearLayout>
            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
                android:background="#A3E7FF"
            >
                <TextView
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/gabarito"
                    android:padding="10dp"
                    android:text="Backlog"
                    android:textSize="16dp"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/backlog"
                    android:textSize="16dp"
                    android:fontFamily="@font/gabarito"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:layout_weight="0.5"
                    android:background="@color/white"
                    />

        </LinearLayout>
            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
                android:background="#A3E7FF"
            >
                <TextView
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/gabarito"
                    android:padding="10dp"
                    android:text="Revenue(Original)"
                    android:textSize="16dp"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/revoriginal"
                    android:textSize="16dp"
                    android:fontFamily="@font/gabarito"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:layout_weight="0.5"
                    android:background="@color/white"
                    />

        </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="#A3E7FF"
                >
                <TextView
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/gabarito"
                    android:padding="10dp"
                    android:text="Revenue(Multiplied)"
                    android:textSize="16dp"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/revmultiplied"
                    android:textSize="16dp"
                    android:fontFamily="@font/gabarito"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:layout_weight="0.5"
                    android:background="@color/white"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="#A3E7FF"
                >
                <TextView
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/gabarito"
                    android:padding="10dp"
                    android:text="Revenue Attainment"
                    android:textSize="16dp"
                    android:textStyle="bold"/>
                <TextView
                    android:id="@+id/revattainment"
                    android:textSize="16dp"
                    android:fontFamily="@font/gabarito"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:layout_weight="0.5"
                    android:background="@color/white"
                    />
            </LinearLayout>


    </LinearLayout>

    </LinearLayout>

</androidx.cardview.widget.CardView>